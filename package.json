{
  "name": "typescriptfullstackshell",
  "version": "1.0.0",
  "description": "demonstrate how to do full-stack TypeScript development",
  "main": "src/server/index.js",
  "scripts": {
    "start": "npm install --only=prod && NODE_ENV=production node ./build/index.js",
    "start:server": "nodemon --config \"./nodemon.json\"/",
    "start:client": "cd ./src/client && npm run start",
    "build:client": "cd ./src/client && npm run build",
    "test:client": "cd ./src/client && npm run test",
    "test": "ts-node ./src/index.ts test",
    "build": "sh ./scripts/buildForProd.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seanpmaxwell/TypeScriptFullStackShell.git"
  },
  "keywords": [
    "TypeScript",
    "Express",
    "React",
    "FullStack",
    "Professional",
    "Enterprise",
    "Startup"
  ],
  "author": "Sean Maxwell",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seanpmaxwell/TypeScriptFullStackShell/issues"
  },
  "homepage": "https://github.com/seanpmaxwell/TypeScriptFullStackShell#readme",
  "dependencies": {
    "express": "^4.16.4"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/node": "^11.10.5",
    "nodemon": "^1.18.10",
    "ts-node": "^8.0.3",
    "tsc": "^1.20150623.0",
    "tslint": "^5.13.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.3333"
  }
}
